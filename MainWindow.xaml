<Window x:Class="_304555HelloWorld.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_304555HelloWorld"
        mc:Ignorable="d"
        Title="meme review" Height="350" Width="600">
    <StackPanel  Orientation="Vertical">
        <Label x:Name="Message" Content="Clap Clap" />
        <TextBox x:Name="txtName" Text="Enter your name here..."/>
        <Button x:Name="Click" Content="Click for greeting" Click="buttonGreeting_Click"/>
    </StackPanel>
</Window>
